{
    "$id": "https://sec.companies.apievangelist.com/companies.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "title": "companies",
        "description": "All the companies with a CIK and industry code.",
        "type": "object",
        "properties": {
            "id": {
                "type": "integer",
                "maxLength": 11
            },
            "CIK": {
                "type": "string",
                "maxLength": 100
            },
            "name": {
                "type": "string",
                "maxLength": 500
            },
            "Location": {
                "type": "string",
                "maxLength": 5
            },
            "SIC": {
                "type": "integer",
                "maxLength": 11
            },
            "SICDescription": {
                "type": "string",
                "maxLength": 250
            },
            "fiscalYearEnd": {
                "type": "string",
                "maxLength": 50
            },
            "stateOfIncorporation": {
                "type": "string",
                "maxLength": 5
            },
            "pull_profile": {
                "type": "integer",
                "maxLength": 11
            },
            "pull_filings": {
                "type": "integer",
                "maxLength": 11
            }
        },
        "required": [
            "id"
        ]
    }
}
